{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config configs/webpack.dev.config.js --progress --colors --hot --inline",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=dev webpack --config configs/webpack.dev.config.js --progress --profile --bail",
    "build:prod": "cross-env NODE_ENV=prod webpack --config configs/webpack.prod.config.js --progress --profile --bail",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elsaywang/react-ts-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elsaywang/react-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/elsaywang/react-ts-boilerplate#readme",
  "dependencies": {
    "awesome-typescript-loader": "^3.4.0",
    "babel": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "configs/webpack.js"
          }
        ]
      ],
      "presets": [
        "react",
        "es2015",
        "stage-0"
      ]
    },
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "node-sass": "^4.7.1",
    "path": "^0.12.7",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
